{
  "name": "whisper-tnode",
  "version": "1.1.0",
  "description": "library for use with whisper.cpp and nodejs or typescript project",
  "main": "dist/index.js",
  "bin": {
    "whisper-tnode": "dist/cli.js"
  },
  "scripts": {
    "prepare": "ts-patch install -s",
    "dev": "cross-env NODE_ENV=development node --watch -r @swc-node/register src/index.ts",
    "cli": "cross-env NODE_ENV=development node -r @swc-node/register src/cli.ts",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "build": "rm -rf dist && tsc",
    "changeversion": "node ./scripts/changeversion.js",
    "test": "cross-env NODE_ENV=development jest --config jest.config.js"
  },
  "private": false,
  "keywords": [
    "cli",
    "cac",
    "picocolors",
    "whisper",
    "whisper-tnode",
    "typescript",
    "shelljs",
    "eslint",
    "prettier"
  ],
  "author": {
    "name": "Juanestban",
    "url": "https://github.com/Juanestban"
  },
  "repository": {
    "url": "https://github.com/Juanestban/whisper-tnode"
  },
  "bugs": "https://github.com/Juanestban/whisper-tnode/issues",
  "license": "ISC",
  "engines": {
    "node": ">=18.11.0"
  },
  "dependencies": {
    "cac": "6.7.14",
    "cross-env": "7.0.3",
    "minimist": "1.2.8",
    "picocolors": "1.0.0",
    "shelljs": "0.8.5"
  },
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@swc-node/jest": "1.6.8",
    "@swc-node/register": "1.6.8",
    "@types/jest": "29.5.6",
    "@types/minimist": "1.2.4",
    "@types/node": "20.8.7",
    "@types/shelljs": "0.8.14",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard-with-typescript": "39.1.1",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-n": "15.0.0 || 16.0.0 ",
    "eslint-plugin-promise": "6.0.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "15.0.2",
    "prettier": "3.0.3",
    "ts-jest": "29.1.1",
    "ts-patch": "3.0.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2",
    "typescript-transform-paths": "3.4.6"
  }
}
